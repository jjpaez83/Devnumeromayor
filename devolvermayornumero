package devnumeromayor;

import java.util.Scanner;

public class Devnumeromayor {

    public static void main(String[] args) {
        // Introducir 2 numeros y devuelve el mayor 

        //Variables
        int n1, n2, m = 0;

        //Entrada
        Scanner teclado = new Scanner(System.in);
        System.out.print("Numero 1: ");
        n1 = teclado.nextInt();
        System.out.print("Numero 2: ");
        n2 = teclado.nextInt();

        //Proceso
        if (n1 > n2) {
            m = n1;
        }

        if (n2 > n1) {
            m = n2;
        }

        //Salida
        System.out.println("");
        System.out.println("El n√∫mero mayor es el  " + m);
    }

}
